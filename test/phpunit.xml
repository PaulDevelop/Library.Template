<?xml version="1.0" encoding="UTF-8"?>

<phpunit
        bootstrap="bootstrap.php"
        >
    <logging>
        <log type="coverage-html"
             target="../report/test/"
             charset="UTF-8"
             highlight="false"
             lowUpperBound="35"
             highLowerBound="70"/>
        <log type="coverage-clover"
             target="../report/test/clover.xml"/>
    </logging>

    <filter>
    </filter>

    <testsuites>
        <testsuite name="Library.Template">
            <directory suffix=".php">.</directory>
        </testsuite>
    </testsuites>
</phpunit>
